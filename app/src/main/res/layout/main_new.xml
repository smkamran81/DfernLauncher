    </LinearLayout>
    <!-- Bildeinstellungen -->
      <TextView
        android:id="@+id/threedot_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bildeinstellungen"
        android:textSize="24sp"
        android:textColor="#85939B"
        android:paddingLeft="30dip"
        android:visibility="gone"  />
    
    </LinearLayout>
      <LinearLayout
            android:id="@+id/screen_btn"
            android:layout_width="fill_parent"
            android:layout_height="100dip"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:paddingLeft="20dip"
            android:clickable="true"
            android:focusable="true">
<LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/screen"
            android:gravity="center"
            android:orientation="vertical"
             >
            
    </LinearLayout>
    <!-- Weitere Einstellungen -->
      <TextView
        android:id="@+id/screen_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weitere Einstellungen"
        android:textSize="24sp"
        android:textColor="#85939B"
        android:paddingLeft="30dip"
        android:visibility="gone"  />
    
    </LinearLayout>
      <LinearLayout
            android:id="@+id/astril_btn"
            android:layout_width="fill_parent"
            android:layout_height="100dip"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:paddingLeft="20dip"
            android:clickable="true"
            android:focusable="true">
<LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/astrill"
            android:gravity="center"
            android:orientation="vertical"
             >
    </LinearLayout>
      <TextView
        android:id="@+id/astril_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Astril VPN"
        android:textSize="24sp"
        android:textColor="#85939B"
        android:paddingLeft="30dip"
        android:visibility="gone"  />
    
    </LinearLayout>
      <LinearLayout
            android:id="@+id/remote_btn"
            android:layout_width="fill_parent"
            android:layout_height="100dip"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:paddingLeft="20dip"
            android:clickable="true"
            android:focusable="true">
          <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/remote"
            android:gravity="center"
            android:orientation="vertical"
             >
            
    </LinearLayout>
    <!-- Remote support nosov-->
      <TextView
        android:id="@+id/remote_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Remote Support"
        android:textSize="24sp"
        android:textColor="#85939B"
        android:paddingLeft="30dip"
        android:visibility="gone"  />
    
    </LinearLayout>
      <LinearLayout
            android:id="@+id/reset_btn"
            android:layout_width="fill_parent"
            android:layout_height="100dip"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:paddingLeft="20dip"
            android:clickable="true"
            android:focusable="true">
<LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/reset"
            android:gravity="center"
            android:orientation="vertical"
             >
            
    </LinearLayout>
    <!-- Factory Reset -->
      <TextView
        android:id="@+id/reset_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Factory Reset"
        android:textSize="24sp"
        android:textColor="#85939B"
        android:paddingLeft="30dip"
        android:visibility="gone"  />
    
    </LinearLayout>
      
      
  </LinearLayout>

</FrameLayout>